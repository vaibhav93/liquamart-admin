      "permission": "ALLOW",
      "property": "loginWithAccessTokenfb"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW",
      "property": "loginWithAccessTokenGoogle"
    },
    {
